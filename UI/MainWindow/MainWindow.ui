/*选择控件*/
QRadioButton {
    spacing: 10px;
    color: black;
    background-color:  rgba(255, 255, 255,0);
}
QRadioButton::indicator {
    background-color: rgba(255, 255, 255,0);
}
QRadioButton::indicator:unchecked {
    height: 18px;
    width: 18px;
    border: 2px solid rgb(154, 154, 154);
    border-radius: 10px;
}
QRadioButton::indicator:checked {
    height: 8px;
    width: 8px;
    border: 7px solid rgb(42, 130, 228);
    border-radius: 10px;
}
QRadioButton::indicator:unchecked:hover {
    height: 18px;
    width: 18px;
    border: 2px solid rgb(120, 120, 120);
    border-radius: 10px;
}
QRadioButton::indicator:checked:hover {
    height: 8px;
    width: 8px;
    border: 7px solid rgb(47, 144, 255);
    border-radius: 10px;
}
QRadioButton::indicator:unchecked:pressed {
    height: 18px;
    width: 18px;
    background-color: rgb(153, 153, 153);
    border: 2px solid rgb(84, 84, 84);
    border-radius: 10px;
}
QRadioButton::indicator:checked:pressed {
    height: 8px;
    width: 8px;
    border: 7px solid rgb(83, 163, 255);
    border-radius: 10px;
}



/*QSlider*/
QSlider::groove:horizontal {
	/*滑条*/
    border: 1px solid #bbb;
    background: white;
    height: 3px;
    border-radius: 1px;
}
  
QSlider::sub-page:horizontal {
	/*已经划过的*/
    background: rgb(0, 150, 255);
    border: 1px solid rgba(255, 255, 255, 0);
    height: 3px;
    border-radius: 1px;
}

QSlider::add-page:horizontal {
	/*没有划过的*/
    background: rgb(235, 235, 235);  
    border: 1px solid rgba(255, 255, 255, 0);
    height: 3px;
    border-radius: 1px;
}
 
QSlider::handle:horizontal {
	/* 上下边距和左右边距(中间的“提手”)*/
    background: rgb(0, 150, 255);
    border: 5px solid rgb(0, 150, 255);
    width: 8px;
    margin-top: -8px;
    margin-bottom: -8px;
    border-radius: 9px;
}

QSlider::handle:horizontal:hover {
    background: rgb(0, 150, 255);
    border: 5px solid rgb(0, 150, 255);
    border-radius: 9px;
}

QSlider::sub-page:horizontal:disabled {
    background: #bbb;
    border-color: #999;
}

QSlider::add-page:horizontal:disabled {
    background: #eee;
    border-color: #999;
}

QSlider::handle:horizontal:disabled {
    background: #eee;
    border: 1px solid #aaa;
    border-radius: 1px;
}


/*QSpinBox*/
QSpinBox {
	/* 为箭头保留空间 */
	padding-right:25px;
	border:2px solid rgb(33, 33, 33);
	background: rgba(255, 255, 255, 128);
	min-width:40px;
	border-radius: 5px;
	padding: -1;  /*改变焦点框*/
}

QSpinBox:focus{
	/*出现光标后*/
	border:2px solid rgb(4, 51, 255);
}


/* 向上按钮 */
QSpinBox::up-button {
	subcontrol-origin:border;
	subcontrol-position:top right;
	width:18px;
	border-image:url(:/widget_Sidebar/images/Up.png);
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0.985075, y2:0, stop:0 rgba(235, 235, 235, 255), stop:1 rgba(255, 255, 255, 0));
	border-top-right-radius:5px;
	border-width:1px;
}
 
/* 向上按钮 */
QSpinBox::up-button:hover {
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:0, stop:0 rgba(225, 225, 225, 255), stop:1 rgba(255, 255, 255, 0));
}
 
/* 向上按钮 */
QSpinBox::up-button:pressed {
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:1, y2:0, stop:0 rgba(190, 190, 190, 255), stop:1 rgba(255, 255, 255, 0));
}
 
/* 向上按钮里的小箭头 */
QSpinBox::up-arrow {
	image:url(:/images/333.bmp);
	width:7px;
	height:7px;
}
 
/* 向下按钮 */
QSpinBox::down-button {
	subcontrol-origin:border;
	subcontrol-position:bottom right;
	width:18px;
	border-image:url(:/widget_Sidebar/images/Dowe.png);
	border-bottom-right-radius:5px;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(235, 235, 235, 255), stop:1 rgba(255, 255, 255, 0));
	border-width:1px;
	border-top-width:0;
}
 
/* 向下按钮 */
QSpinBox::down-button:hover {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(225, 225, 225, 255), stop:1 rgba(255, 255, 255, 0));
}
 
/* 向下按钮 */
QSpinBox::down-button:pressed {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(190, 190, 190, 255), stop:1 rgba(255, 255, 255, 0));
}
 
/* 向下按钮里的小箭头 */
QSpinBox::down-arrow {
	image:url(:/images/333.bmp);
	width:7px;
	height:7px;
}

